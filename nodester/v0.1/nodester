	'npmInstall': function(args, opts) {
		var url = 'http://api.nodester.com//npm'
		args = args || {}
		if (args['action'] == null) args['action'] = "install"
		var rgx = new RegExp('\{action\}', 'g')
		if (url.match(rgx)) {
			url = url.replace(rgx, args['action'])
			if (args['action']) delete args['action']
