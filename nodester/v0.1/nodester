		if (url.indexOf('[/action]') !== -1) url = url.replace('[/action]', '')
		if (url.indexOf('[/appname]') !== -1) url = url.replace('[/appname]', '')
		if (url.indexOf('[/package]') !== -1) url = url.replace('[/package]', '')
