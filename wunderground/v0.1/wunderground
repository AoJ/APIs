({
	auth:function(){return auth({provider:'wunderground'})},
	'USA_GeolocateExample': function(args, opts) {
		var url = 'http://api.wunderground.com/api//{key}/geolookup/q/CA/San_Francisco.json'
		args = args || {}
		return http({provider:'wunderground',url:url,method:'GET',params:args, auth:'none'}, opts)
	},
